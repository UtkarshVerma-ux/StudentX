{
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/submitIssue", "dest": "/api/submitIssue.js" },
    { "src": "/api/(.*)\\.js", "dest": "/api/$1.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
